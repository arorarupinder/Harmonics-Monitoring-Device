import numpy as np #improted numpy as alias np
A= np.arange(1, 50, 2.5) #another way of creating an array ,1st par=init val,2nd par=fin val, 3rd par=incr.
B= np.arange(25, 75, 1)
C= np.arange(0, 65, 5)
#print("Array A: ", A) 
#print("Array B: ", B)
#print("Array C: ", C)
max1= A.max()
max2= B.max()
max3= C.max()
print ("Max number is:", max1, max2, max3)

D= np.array([max1, max2, max3])
max4= D.max()
print ("Max number is:", max4)
